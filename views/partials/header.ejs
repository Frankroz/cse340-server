<header id="top-header">
  <div class="site-logo">
    <a href="/" title="Home Page">
      <img src="/images/site/icon.png" alt="CSE Motors Logo" />
      <h1>CSE Motors</h1>
    </a>
  </div>
  <div class="site-header-content">
    <% if (locals.loggedin) { %>
    <a href="/account" title="Account Management" id="welcome-link"
      >Welcome <%= locals.accountData.account_firstname %></a
    >

    <a href="/account/logout" title="Log out" id="logout-link">Logout</a>
    <% } else { %>
    <a href="/account/login" title="Account Management" id="my-account-link"
      >My Account</a
    >
    <% } %>
  </div>
</header>
